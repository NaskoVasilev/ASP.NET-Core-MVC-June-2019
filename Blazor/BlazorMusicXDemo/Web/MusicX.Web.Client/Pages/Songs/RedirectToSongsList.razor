@page "/songslistredirect/"
@page "/songslistredirect/{SearchTerms}"
@page "/songslistredirect/{SearchTerms}/{PageNumber:int}"
@inject IUriHelper UriHelper

@code {

    [Parameter]
    private int PageNumber { get; set; } = 1;

    [Parameter]
    private string SearchTerms { get; set; } = null;

    protected override void OnInit()
    {
        this.UriHelper.NavigateTo($"/songslist/{this.SearchTerms}/{this.PageNumber}");
    }
}
